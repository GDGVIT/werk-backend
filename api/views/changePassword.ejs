<body>
      <div class="container mt-5 mx-auto px-auto">
        <h1 class="display-5 text-center">WERK</h1>
      <form class="shadow py-5 rounded" action="<%=url%>" method="POST">
        <h1 class="text-center mb-5 display-5">Enter new password</h1>
        <div class="row px-5 mb-5">
          <div class="col-sm-3 col-md-6">
              <label for="inputPassword" class="form-label">Password</label>
              <div id="passwordHelp" class="form-text"><label for="inputPassword" class="form-label">Min. 5 characters</label></div>
              <input type="text" name="<%=userId%>" id="inputPassword" class="form-control" aria-describedby="passwordHelpInline">
          </div>
        </div>
        <div class="row px-5">
          <div class="col-sm-1 col-md-3">
            <button disabled="true" id="submitButton" type="submit" class="btn btn-primary">change password</button>
          </div>
        </div>
       </form>
       </div>
        <script src="/scripts/index.js"></script>
</body>
